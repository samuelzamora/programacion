#include "stdafx.h"
#include<iostream>
#include"conio.h"

using namespace std;
 

void main() {
	float radio,area;
	cout<<"ingresar radio: ";
	cin>>radio;
	area=3.141516*radio*radio;
	cout<< "el area de la circunferencia es: " <<area;
	getch();
}
